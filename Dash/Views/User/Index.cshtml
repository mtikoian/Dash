@using Dash.I18n
@model Table

<div data-title="@Users.ViewAll" data-basic-dialog="true" class="container">
    <div class="p-2">
        @using (Html.BeginToolbar())
        {
            @Html.AuthorizedButton(Users.CreateUser, "User", "Create", DashClasses.BtnPrimary, DashClasses.DashDialog, hasAccess: this.User.IsInRole("user.create"))
        }
        <div class="col-12 dash-table" data-json="@Model.ToJson"></div>
    </div>
</div>
