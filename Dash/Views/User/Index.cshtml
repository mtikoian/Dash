@using Dash.Resources
@model Table

<body-content>
    <breadcrumb>
        <breadcrumb-item label="@Users.ViewAll" controller="User" action="Index" is-active="true"></breadcrumb-item>
    </breadcrumb>

    <partial name="_Toast" />

    <toolbar no-render="@(!User.IsInRole("user.create"))">
        @Html.AuthorizedButton(Users.CreateUser, "Create", "User", null, DashClasses.BtnPrimary)
    </toolbar>

    <dynamic-table options="@Model"></dynamic-table>
</body-content>
